{
    "name": "@Delciojr",
    "version": "1.0",
    "description": "@Delciojr",
    "manifest_version": 3,
    "background":{
        "service_worker":"config.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
      },
      "host_permissions": [
       "..."
      ],
      "permissions": [
        "tabGroups",
        "tabs",
        "scripting",
        "storage",
        "activeTab"
      ]
  }